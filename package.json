{
  "name": "ilmestys",
  "version": "1.0.0",
  "description": "RSS feed generator for GitHub notifications",
  "packageManager": "yarn@3.2.1",
  "type": "module",
  "dependencies": {
    "@koa/router": "^10.1.1",
    "feed": "4.2.2",
    "koa": "2.13.4",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/koa": "^2.11.3",
    "@types/koa__router": "^8.0.11",
    "prettier": "2.7.1",
    "tsc-watch": "^5.0.3",
    "typescript": "4.7.4"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc-watch --onSuccess \"yarn run start\"",
    "format": "prettier -w .",
    "start": "node ./dist/index.js"
  }
}
